{
  "name": "interactive-program-repair",
  "version": "0.0.0",
  "description": "Interactive program repair in JavaScript",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf lib && mkdirp lib",
    "build": "babel src -d lib",
    "prebrowserify": "rimraf dist && mkdirp dist",
    "browserify": "browserify src/index.js --standalone default -t babelify --outfile dist/index.js",
    "prepublishOnly": "npm run build && npm run browserify"
  },
  "author": "Michael Jae-Yoon Chung",
  "license": "MIT",
  "repository": "https://gitlab.com/mjyc/interactive-program-repair",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "rimraf": "^2.6.3"
  }
}
