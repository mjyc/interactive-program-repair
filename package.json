{
  "private": true,
  "scripts": {
    "build": "browserify src/index.js -t babelify --outfile dist/index.js -dv",
    "watch": "mkdirp dist && watchify src/index.js -t babelify --outfile dist/index.js -dv",
    "live-server": "live-server --watch=./dist/index.js,./index.html",
    "prestart": "test -e src/settings.json || echo {} > src/settings.json",
    "start": "npm-run-all --parallel watch live-server"
  },
  "author": "Michael Jae-Yoon Chung",
  "license": "MIT",
  "dependencies": {
    "@cycle-robot-drivers/run": "1.0.25",
    "@cycle-robot-drivers/screen": "1.1.7",
    "@cycle/dom": "22.3.0",
    "@cycle/http": "15.1.0",
    "@cycle/run": "5.2.0",
    "@cycle/state": "1.3.0",
    "@cycle/time": "0.19.0",
    "@mjyc/cycle-time-travel": "0.1.4",
    "xstream": "11.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "live-server": "^1.2.0",
    "mkdirp": "~0.5.0",
    "npm-run-all": "^4.1.5",
    "watchify": "^3.11.0"
  }
}
